                     <xs:enumeration value="column">
                        <xs:annotation>
                           <xs:documentation>column beginnings.</xs:documentation>
                        </xs:annotation>
                     </xs:enumeration>
                  </xs:restriction>
               </xs:simpleType>
               <xs:simpleType>
                  <xs:restriction base="xs:token">
