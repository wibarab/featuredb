   <text>
      <body>
         <div type="fsLibrary">
            <!-- Here the types of the feature value are listed and can be broken down into
                 its formal components -->
            <fvLib n="q featureValues" xml:id="q_values">
               <fs xml:id="q_g"></fs>
               <fs xml:id="q_q"></fs>
               <fs xml:id="q_g_affricated"></fs>
               <fs xml:id="q_hamza"></fs>
            </fvLib>
         </div>
     
         <div type="description">
           <p><!-- Verbose discussion of the feature --></p>
         </div>

         <div type="featureRealisations">
            
            
            <fs type="featureRealisation" source="#younes2014">
               <f name="dialect" fVal="dialects:Basra"/>
               
              
               <f name="realisationType" fVal="#q_g"/>
               <f name="example" fVal="#ex_2929"/>
               
               <f name="attestedPlace" fVal="geo:888"/>
               <f name="attestedDate">
                  <vColl>
                     <symbol value="yearOf"/>
                  </vColl>
               </f>
               
               <f name="speakers">
                  <fs>
                     <f name="gender"/>
                     <f name="ageGroup"/>
                     <f name="academicLevel"/>
                     <f name="occupation"/>
                     <f name="note">
                        <string></string>
                     </f>
                  </fs>
               </f>
            </fs>

            <fs type="featureInstantiation" source="#younes2014">
               <f name="featureValue" fVal="#q_g"/>
               <f name="dialect" fVal="#abu_ciid">/</f>
               <f name="example">
                  <fs>
                     <f name="wordform"><string xml:lang="ar">nǝlga</string></f>
                     <f name="trans"><string xml:lang="en">we find</string></f>
                     <f name="msd" fVal="#v_pres_pl_p1"/>
                  </fs>
               </f>
               <f name="pageNumber"><string>IV</string></f>
            </fs>

            <fs type="featureInstantiation" source="#younes2014">
               <f name="featureValue" fVal="#q_g"/>
               <f name="dialect" fVal="#abu_ciid"/>
               <f name="example" fVal="#s_2929"/>
               
               
               <!-- translation + msd > example ?  -->
               <f name="trans"><string xml:lang="en">we find</string></f>
               <f name="msd" fVal="#v_pres_pl_p1"/>
               
               <f name="pageNumber"><string>IV</string></f>
               
            </fs>

            <fs type="featureInstantiation" source="#behnstedt2001">
               <f name="featureValue" fVal="#q_g"></f>
               <f name="dialect" fVal="#abu_cali"></f>
               
               <f name="pageNumber"><string>IV</string></f>
            </fs>
         </div>

         <div type="examples">
            <cit type="example" xml:id="ex_2929">
               <quote>
                  <s xml:id="s_2929_1">ana nǝl<seg ana="#q_g">g</seg>ah fi s-su<seg ana="#q_q">q</seg>.</s>
               </quote>
               <cit type="translation">
                  <quote></quote>
               </cit>
            </cit>
            <cit type="example" xml:id="ex_2041">
               <quote>
                  <w ana="#n_sg_f">ḅ<seg ana="#q_g">g</seg>aṛa</w>
               </quote>
               <cit type="translation">
                  <quote>cow</quote>
               </cit>
            </cit>
         </div>
      </body>
