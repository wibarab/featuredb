      <ref name="tei_genName"/>
   </define>
   <define name="tei_model.persNamePart_sequenceOptional">
      <optional>
         <ref name="tei_forename"/>
      </optional>
      <optional>
         <ref name="tei_genName"/>
      </optional>
   </define>
   <define name="tei_model.persNamePart_sequenceOptionalRepeatable">
