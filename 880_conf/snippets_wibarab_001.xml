<snippets xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:wib="https://wibarab.acdh.oeaw.ac.at/langDesc" xml:space="preserve">

   <!--*************************************************************************-->
   <!--** The list on the left side is filled with the pseudo-n (pn)        ****-->
   <!--** and pseudo-name (pname) attributes.                               ****-->
   <!--** Pseudo-n's can also be used as substittion short-cuts in the XML  ****-->
   <!--** editor. E.g. #p# is automatically replaced by <p></p>             ****-->
   <!--*************************************************************************-->

<wib:featureValueObservation pn="fv" pname="featValueObs" status="draft" xml:id="" resp="">
               <name type="featureValue" ref=""/>
               <bibl corresp="" type="publication"/>
               <placeName ref=""/>
               <lang target=""/>
               <personGrp role="" corresp=""/>
               <cit type="example" xml:id=""><quote></quote>
                  <cit type="translation"><quote></quote></cit>
               </cit>
               <note type="general">1</note>
               <note type="constraintNote">2</note>
               <note type="exceptionNote">3</note>
            </wib:featureValueObservation>

            <cit pn="sr" pname="sourceRepr" type="sourceRepresentation"><quote></quote></cit>

            <personGrp pn="pg" pname="personGrp" role="" corresp=""/>

<change pn="ch" pname="change" when="{iso-date}" who="dmp:???">{caret}</change>

<cit pn="ci" pname="cit:example" type="example" xml:id="ex_{rn}">
                     <quote>{caret}</quote>
                     <cit type="translation">
                        <quote> </quote>
                     </cit>
                  </cit>

<cit pn="oc" pname="orCit" type="orCit" xml:id="os_{rn}" source="">
   <quote></quote>
</cit>

<foreign pn="f" pname="foreign">{insert}</foreign>{caret}

<lang pn="la" pname="profileLink" target="aProfile{caret}"/>

<note pn="nc" pname="note:constraint" type="constraintNote">{caret}</note>

<note pn="ne" pname="note:exception" type="exceptionNote">{caret}</note>

<note pn="ng" pname="note:general" type="general">{caret}</note>

<personGrp pn="tr" pname="personGrp:tr" role="tribe" xml:id="">
   <name>[tribe's name]</name>
   <residence>
      <placeName ref=""/>
   </residence>
</personGrp>

<personGrp pn="ra" pname="relAffiliation" type="religousAffiliatian" corresp="{caret}"/>

<lang pn="lp" pname="lang:profile" target="{caret}"/>

<language pn="la" pname="language" ident="ar" ana="{caret}"/>


   <!--*************************************************************************-->
   <!--**   Templates used in XSL **********************************************-->
   <!--*************************************************************************-->

   <xsl:apply-templates/>{caret}

   <xsl:attribute name="{caret}"></xsl:attribute>

   <xsl:choose>{caret}</xsl:choose>

   <xsl:for-each select="{caret}"></xsl:for-each>

   <xsl:if test="">{caret}</xsl:if>

   <xsl:otherwise>{caret}</xsl:otherwise>

   <xsl:text>{caret}</xsl:text>

   <xsl:template match="{caret}"></xsl:template>

   <xsl:value-of select="{caret}"/>

   <xsl:variable></xsl:variable>

   <xsl:when test="">{caret}</xsl:when>


</snippets>
