<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" targetNamespace="https://id.acdh.oeaw.ac.at/langDesc" xmlns:langdesc="https://id.acdh.oeaw.ac.at/langDesc" xmlns:tei="http://www.tei-c.org/ns/1.0">
  <xs:import namespace="http://www.tei-c.org/ns/1.0" schemaLocation="featuredb.xsd.tmp"/>
  <xs:import namespace="http://www.w3.org/XML/1998/namespace" schemaLocation="xml.tmp"/>
  <xs:element name="listFeatureValues">
    <xs:annotation>
      <xs:documentation>(Feature Value List) lists possible values a feature can be realised</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="langdesc:featureValue"/>
        <xs:element ref="langdesc:featureValue"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="langdesc:featureValue"/>
      </xs:sequence>
      <xs:attributeGroup ref="tei:tei_att.global.attribute.xmlid"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="featureValue">
    <xs:annotation>
      <xs:documentation>(feature value) describes one feature value</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="tei:name"/>
        <xs:element maxOccurs="unbounded" ref="tei:p"/>
        <xs:choice>
          <xs:element minOccurs="0" maxOccurs="unbounded" ref="tei:note"/>
          <xs:element minOccurs="0" maxOccurs="unbounded" ref="tei:cit"/>
        </xs:choice>
      </xs:sequence>
      <xs:attributeGroup ref="tei:tei_att.global.attribute.xmlid"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="featureValueObservation" substitutionGroup="tei:tei_model.divLike">
    <xs:annotation>
      <xs:documentation>(feature value observation) contains data gathered by a source when observing a given feature</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="tei:biblScope"/>
        <xs:element minOccurs="0" ref="tei:settingDesc"/>
        <xs:element minOccurs="0" ref="tei:particDesc"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="tei:cit"/>
        <xs:element ref="tei:note"/>
      </xs:sequence>
      <xs:attributeGroup ref="tei:tei_att.global.attributes"/>
      <xs:attribute name="fVRef" use="required" type="xs:anyURI"/>
    </xs:complexType>
  </xs:element>
</xs:schema>
