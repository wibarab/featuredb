<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" targetNamespace="https://wibarab.acdh.oeaw.ac.at/langDesc" xmlns:langdesc="https://id.acdh.oeaw.ac.at/langDesc" xmlns:dcr="http://www.isocat.org/ns/dcr" xmlns:l="https://wibarab.acdh.oeaw.ac.at/langDesc" xmlns:tei="http://www.tei-c.org/ns/1.0">
  <xs:import namespace="http://www.isocat.org/ns/dcr" schemaLocation="dcr.tmp"/>
  <xs:import namespace="http://www.tei-c.org/ns/1.0" schemaLocation="featuredb.xsd.tmp"/>
  <xs:import namespace="http://www.w3.org/XML/1998/namespace" schemaLocation="xml.tmp"/>
  <xs:import namespace="https://id.acdh.oeaw.ac.at/langDesc" schemaLocation="langdesc.tmp"/>
  <xs:element name="featureValueObservation">
    <xs:annotation>
      <xs:documentation>(feature value observation) contains data gathered by a source about the realisation of a given feature</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="langdesc:featureValue"/>
        <xs:element maxOccurs="unbounded" ref="tei:bibl"/>
        <xs:choice>
          <xs:element minOccurs="0" ref="tei:region"/>
          <xs:element minOccurs="0" ref="tei:settlement"/>
        </xs:choice>
        <xs:element ref="tei:date"/>
        <xs:sequence>
          <xs:element ref="tei:name"/>
          <xs:element minOccurs="0" ref="tei:name"/>
        </xs:sequence>
        <xs:element minOccurs="0" ref="tei:lang"/>
        <xs:element maxOccurs="unbounded" ref="tei:personGrp"/>
        <xs:choice>
          <xs:element minOccurs="0" maxOccurs="unbounded" ref="tei:cit"/>
          <xs:element minOccurs="0" maxOccurs="unbounded" ref="tei:ptr"/>
        </xs:choice>
        <xs:element maxOccurs="unbounded" ref="tei:note"/>
      </xs:sequence>
      <xs:attributeGroup ref="tei:tei_att.global.attribute.xmllang"/>
      <xs:attributeGroup ref="tei:tei_att.global.attribute.xmlbase"/>
      <xs:attributeGroup ref="tei:tei_att.global.attribute.xmlspace"/>
      <xs:attributeGroup ref="tei:tei_att.global.responsibility.attribute.cert"/>
      <xs:attributeGroup ref="tei:tei_att.global.responsibility.attribute.resp"/>
      <xs:attributeGroup ref="tei:tei_att.global.responsibility.attributes"/>
      <xs:attributeGroup ref="tei:tei_att.docStatus.attributes"/>
      <xs:attribute ref="xml:id" use="required"/>
    </xs:complexType>
  </xs:element>
</xs:schema>
